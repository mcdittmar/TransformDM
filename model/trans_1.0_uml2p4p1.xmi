      <ownedEnd xmi:type="uml:Property" xmi:id="_TgQV1f3BEe62iMPHlPSlUQ" visibility="public" type="_TgQVy_3BEe62iMPHlPSlUQ" association="_TgQV1P3BEe62iMPHlPSlUQ"/>
