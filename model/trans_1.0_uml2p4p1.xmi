    <packagedElement xmi:type="uml:Class" xmi:id="_TgQV1v3BEe62iMPHlPSlUQ" name="PermuteAxis">
      <ownedComment xmi:type="uml:Comment" xmi:id="_TgQV1_3BEe62iMPHlPSlUQ">
        <body>Entry for the Permute operation, this object defines the mapping of input dimension to output dimension.  The output dimension is determined from its order in the axismap list.</body>
      </ownedComment>
      <ownedAttribute xmi:type="uml:Property" xmi:id="_TgQV2P3BEe62iMPHlPSlUQ" name="sourceAxis" visibility="public" type="_TgQWi_3BEe62iMPHlPSlUQ" isUnique="false">
